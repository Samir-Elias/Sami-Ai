{
  "name": "devai-agent-backend",
  "version": "1.0.0",
  "description": "Backend API para DevAI Agent - Asistente de desarrollo con IA",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon src/server.js",
    "start": "node src/server.js",
    "build": "node scripts/build.js",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:generate": "prisma generate",
    "db:seed": "node src/database/seed.js",
    "db:reset": "prisma migrate reset --force",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "lint": "eslint src --ext .js",
    "lint:fix": "eslint src --ext .js --fix",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down"
  },
  "keywords": [
    "ai",
    "development",
    "assistant",
    "chat",
    "api",
    "backend"
  ],
  "author": "DevAI Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "uuid": "^9.0.1",
    "date-fns": "^2.30.0",
    "lodash": "^4.17.21",
    "dotenv": "^16.3.1",
    "prisma": "^5.6.0",
    "@prisma/client": "^5.6.0",
    "redis": "^4.6.10",
    "ioredis": "^5.3.2",
    "node-cron": "^3.0.3",
    "sharp": "^0.32.6",
    "archiver": "^6.0.1",
    "pdf-parse": "^1.1.1",
    "mammoth": "^1.6.0",
    "csv-parser": "^3.0.0",
    "winston": "^3.11.0",
    "express-winston": "^4.2.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "express-async-errors": "^3.1.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "vitest": "^0.34.6",
    "supertest": "^6.3.3",
    "@types/node": "^20.9.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "prisma": {
    "schema": "src/database/schema.prisma"
  }
}